{
    "name": "Focus deeply",
    "description": "Focus deeply and be more productive",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "./options/index.html",
    "action": {
        "default_title": "Click here"
    }
}